set term svg background rgb "white"
set title "u''=u"
set key bottom right
set tics out
set grid
set xzeroaxis
set yzeroaxis
set xlabel "x"
set ylabel "y"
set out "plotA.svg"
plot \
 "plot.txt" u 1:2 with lines title "u(x)"\
,"plot.txt" u 1:3 with lines title "u'(x)"\
,"plot.txt" u 1:4 with lines title "Sin(x)"\
,"plot.txt" u 1:5 with lines title "Cos(x)"\


set term svg background rgb "white"
set title "SIR-model with T_{c}=4"
set key bottom right
set tics out
set grid
set xzeroaxis
set yzeroaxis
set xlabel "Time in days"
set ylabel "Number"
set out "plotT4.svg"
plot \
"data1.txt" u 1:2 with lines title "S"\
,"data1.txt" u 1:3 with lines title "I"\
,"data1.txt" u 1:4 with lines title "R"\


set term svg background rgb "white"
set title "Particle in box"
set key bottom right
set tics out
set grid
set xzeroaxis
set yzeroaxis
set xlabel "x"
set ylabel "Psi"
set out "plotA.svg"
plot \
 "data.txt" u 1:2 with lines title "n=1"\
,"data.txt" u 1:3 with lines title "n=2"\
,"data.txt" u 1:4 with lines title "n=3"\
,"data.txt" u 1:5 with lines title "n=4"\
,"data.txt" u 1:6 with lines title "n=5"\

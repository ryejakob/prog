set term svg background rgb "white"
set title "Calculated epsilon"
set key bottom right
set tics out
set grid
set xzeroaxis
set yzeroaxis
set xlabel "rmax in Bohr radius"
set ylabel "epsilon in Hatree"
set out "plotC.svg"
plot \
"plot1.txt" u 1:2 with lines title "simple boundary"\
, "plot1.txt" u 1:3 with lines title "new boundary"\

